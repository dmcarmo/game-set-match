<%= simple_form_for @availability do |f| %>
  <%= f.input :start_time, as: :date, html5: true, required: true %>
  <%= f.input :end_time, as: :date, html5: true, required: true %>

  <div class="form-group">
    <%= f.submit "Save", class: "btn btn-primary" %>
    <%= link_to 'Cancel', authenticated_root_path, class: "btn btn-secondary" %>
  </div>
<% end %>
